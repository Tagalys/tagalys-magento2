<?xml version="1.0" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
  <group id="tagalys">
    <job instance="Tagalys\Sync\Cron\RunMaintenance" method="tryExecute" name="TagalysMaintenance">
      <config_path>crontab/tagalys/jobs/maintenance/schedule/cron_expr</config_path>
    </job>
    <job instance="Tagalys\Sync\Cron\Sync" method="tryExecute" name="TagalysSync">
      <config_path>crontab/tagalys/jobs/sync/schedule/cron_expr</config_path>
    </job>
    <job instance="Tagalys\Sync\Cron\PositionUpdate" method="tryExecute" name="TagalysPositionUpdate">
      <config_path>crontab/tagalys/jobs/position_update/schedule/cron_expr</config_path>
    </job>
  </group>
</config>
